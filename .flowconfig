[ignore]
src/log-files
src/old

[include]
src

[libs]
# https://github.com/nrnb/GoogleSummerOfCode/issues/94
# cytoscape is getting flow this summer

[lints]
# all=off by default
all=warn

[options]
suppress_comment=\\(.\\|\n\\)*\\$Flow

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
